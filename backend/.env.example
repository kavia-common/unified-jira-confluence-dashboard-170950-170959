# Atlassian OAuth Configuration
# These values need to be obtained from Atlassian Developer Console
ATLASSIAN_CLIENT_ID=your_atlassian_client_id_here
ATLASSIAN_CLIENT_SECRET=your_atlassian_client_secret_here

# OAuth Redirect URIs
# These should match the redirect URIs configured in your Atlassian OAuth app
JIRA_REDIRECT_URI=http://localhost:8000/auth/jira/oauth/callback
CONFLUENCE_REDIRECT_URI=http://localhost:8000/auth/confluence/oauth/callback

# Application Security
SECRET_KEY=your-secret-key-change-in-production
